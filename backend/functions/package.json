{
    "name": "aiuda-chat-functions",
    "version": "1.0.0",
    "description": "AI Chat Backend with Firebase Genkit",
    "main": "lib/index.js",
    "scripts": {
        "build": "tsc && cp -r src/prompts lib/",
        "dev": "genkit start -- tsx --watch src/index.ts",
        "deploy": "npm run build && firebase deploy --only functions"
    },
    "engines": {
        "node": "22"
    },
    "dependencies": {
        "@genkit-ai/ai": "^0.5.0",
        "@genkit-ai/core": "^0.5.0",
        "@genkit-ai/dotprompt": "^0.5.0",
        "@genkit-ai/flow": "^0.5.0",
        "@genkit-ai/googleai": "^0.5.0",
        "@google/generative-ai": "^0.24.1",
        "dotenv": "^17.2.3",
        "firebase-admin": "^12.0.0",
        "firebase-functions": "^5.0.0",
        "zod": "^3.22.4"
    },
    "devDependencies": {
        "@types/node": "^20.0.0",
        "genkit": "^0.5.0",
        "tsx": "^4.7.0",
        "typescript": "^5.3.0"
    }
}